    "yarn-error.log": true,
    ".env": true,
    ".env.*": true,
    ".DS_Store": true,
    "*.log": true,
    ".vscode": true,
    ".idea": true,
    "*.sublime-*": true,
    "coverage": true,
    "*.lcov-report": true,
    "*.nyc_output": true,
    "*.bak": true,
